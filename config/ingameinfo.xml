<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<config>
  <lines at="topleft">
    <!-- Biome -->
    <line>
      <str>Biome: $6{biome}</str>
    </line>
    <!-- Time -->
    <line>
      <str>Time: $6</str>
      <if> <!-- From 24 hour to 12 hour -->
        <less>
          <var>mctimeh</var>
          <num>13</num>
        </less>
        <add> <!-- Useless math operation to remove zero padding -->
          <var>mctimeh</var>
          <num>0</num>
        </add>
        <sub>
          <var>mctimeh</var>
          <num>12</num>
        </sub>
      </if>
      <str>$7:$6{mctimem} $3</str>
      <if> <!-- Get AM if hour is less than 13 -->
        <less>
          <var>mctimeh</var>
          <num>13</num>
        </less>
        <str>AM</str>
        <str>PM</str>
      </if>
    </line>
    <line>
      <str>FPS: </str>
      <op> <!-- Get a color by value that is greater than -->
        <str>lt</str>
        <var>fps</var>
        <num>15</num>
        <num>30</num>
        <num>40</num>
        <num>50</num>
        <str>$4</str> <!-- Dark Red -->
        <str>$c</str> <!-- Red -->
        <str>$6</str> <!-- Gold -->
        <str>$e</str> <!-- Yellow -->
        <str>$a</str> <!-- Green -->
      </op>
      <var>fps</var>
    </line>
    <line>
      <if>
        <greater>
          <var>ping</var>
          <num>0</num>
        </greater>
        <concat>
          <str>Ping: </str>
          <op> <!-- Get a color by value that is less than -->
            <str>gt</str>
            <var>ping</var>
            <num>100</num>
            <num>80</num>
            <num>60</num>
            <num>40</num>
            <str>$4</str> <!-- Dark Red -->
            <str>$c</str> <!-- Red -->
            <str>$6</str> <!-- Gold -->
            <str>$e</str> <!-- Yellow -->
            <str>$a</str> <!-- Green -->
          </op>
          <str>{ping}</str>
        </concat>
      </if>
    </line>
  </lines>
  <lines at="topcenter">
    <!-- Compass Direction -->
    <line>
      <op> <!-- Red for North, Yellow for south, Aqua for east and west -->
        <str>eq</str>
        <var>abrroughdirection</var>
        <str>N</str>
        <str>S</str>
        <str>$c</str>
        <str>$e</str>
        <str>$b</str>
      </op>
      <str>{finedirection} $7- $r</str>
      <round> <!-- Show the degree as heading -->
        <add>
          <var>direction</var>
          <num>180</num>
        </add>
        <num>0</num>
      </round>
      <str>$7Â°</str>
    </line>
  </lines>
  <lines at="topright">
    <!-- Coordinates -->
    <line>
      <str>$7($rX: $b{xi}$7, $rY: $a{yi}$7, $rZ: $b{zi}$7)</str>
    </line>
  </lines>
