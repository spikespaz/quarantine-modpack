  <lines at="bottomright">
    <!-- Mainhand Item -->
    <line>
      <if>
        <greater>
          <var>mainhandmaxdamage</var>
          <num>0</num>
        </greater>
        <concat>
          <str>{mainhanddamageleft}$7/$r{mainhandmaxdamage} $7(</str>
          <op>
            <str>lt</str>
            <pct>
              <var>mainhanddamageleft</var>
              <var>mainhandmaxdamage</var>
            </pct>
            <num>10</num>
            <num>20</num>
            <num>30</num>
            <num>50</num>
            <str>$4</str> <!-- Dark Red -->
            <str>$c</str> <!-- Red -->
            <str>$6</str> <!-- Gold -->
            <str>$e</str> <!-- Yellow -->
            <str>$a</str> <!-- Green -->
          </op>
          <round>
            <pct>
              <var>mainhanddamageleft</var>
              <var>mainhandmaxdamage</var>
            </pct>
            <num>1</num>
          </round>
          <str>%$7) {mainhandlargeicon}</str>
        </concat>
        <if>
          <greater>
            <var>mainhandquantity</var>
            <num>0</num>
          </greater>
          <concat>
            <str>{mainhandname} </str>
            <if>
              <greater>
                <var>mainhandquantity</var>
                <num>1</num>
              </greater>
              <str>$7($b{mainhandquantity}$7) </str>
            </if>
            <var>mainhandlargeicon</var>
          </concat>
        </if>
      </if>
    </line>
    <!-- Offhand Item -->
    <line>
      <if>
        <greater>
          <add>
            <var>offhandmaxdamage</var>
            <var>offhandquantity</var>
          </add>
          <num>0</num>
        </greater>
        <str> </str>
      </if>
    </line>
    <!-- End Spacer -->
    <line>
      <if>
        <greater>
          <var>offhandmaxdamage</var>
          <num>0</num>
        </greater>
        <concat>
          <str>{offhanddamageleft}$7/$r{offhandmaxdamage} $7(</str>
          <op>
            <str>lt</str>
            <pct>
              <var>offhanddamageleft</var>
              <var>offhandmaxdamage</var>
            </pct>
            <num>10</num>
            <num>20</num>
            <num>30</num>
            <num>50</num>
            <str>$4</str> <!-- Dark Red -->
            <str>$c</str> <!-- Red -->
            <str>$6</str> <!-- Gold -->
            <str>$e</str> <!-- Yellow -->
            <str>$a</str> <!-- Green -->
          </op>
          <round>
            <pct>
              <var>offhanddamageleft</var>
              <var>offhandmaxdamage</var>
            </pct>
            <num>1</num>
          </round>
          <str>%$7) {offhandlargeicon}</str>
        </concat>
        <if>
          <greater>
            <var>offhandquantity</var>
            <num>0</num>
          </greater>
          <concat>
            <str>{offhandname} </str>
            <if>
              <greater>
